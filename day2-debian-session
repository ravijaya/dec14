     1	#!/bin/bash
     2	x=13
     3	n=3
     4	echo $(($x + $n))   # arthmetic expansion
     5	echo $((x + n))   # arthmetic expansion
     6	echo 
     7	
     8	power=$((x ** n))
     9	echo "raised $x to the power of $n is $power"
    10	echo
    11	
    12	# syntax  for the conditional operator 
    13	# test-condition? ture-part-expt : false-part-expr
    14	
    15	result=$((x > n ? x ** 2 : x ** 3))  # conditional operator 
    16	echo $result
    17	echo 
    18	echo $((x / 5))
    19	echo $((x % 5))
